<!-- Page Heading -->
<a href="#" data-toggle="modal" data-target="#uploadModal" class="btn btn-primary mb-3">Import Excel</a>

<!-- DataTales Example -->
<div class="card shadow mb-4">
    <div class="card-header py-3">
        <h6 class="m-0 font-weight-bold text-primary">Data Users</h6>
    </div>
    <div class="card-body">
        <div class="table-responsive">
            <table class="table table-bordered" id="dataTable" width="100%" cellspacing="0">
                <thead>
                    <tr>
                        <th>ID</th>
                        <th>Email</th>
                        <th>Role</th>                        
                        <th>Action</th>               
                    </tr>
                </thead>
                <tfoot>
                    <tr>
                        <th>ID</th>
                        <th>Email</th>
                        <th>Role</th>         
                        <th>Action</th>               
                    </tr>
                </tfoot>
                <tbody>                    
                    <% data.forEach(item => { %>
                        <tr>
                            <td><%- item.iditem %> </td>
                            <td><%- item.namaitem %></td>
                            <td><%- item.jumlahitem %></td>
                            <td><a href="/edititem/<%- item.iditem %>" class="btn btn-warning">Edit</a>  <a href="/deleteitem/<%- item.iditem %>" onclick="return confirm('Are U Sure?')" class="btn btn-danger">Delete</a></td>
                        </tr>            
                        <% }) %>
                </tbody>
            </table>
        </div>
    </div>
</div>